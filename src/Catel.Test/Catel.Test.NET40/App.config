<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="catel">
      <section name="ioc" type="Catel.IoC.IoCConfigurationSection, Catel.Core" />
    </sectionGroup>

    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=4.4.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>

  <catel>
    <ioc>
      <serviceLocatorConfigurations>
        <serviceLocatorConfiguration>
          <register interfaceType="Catel.MVVM.Services.IUIVisualizerService, Catel.MVVM" implementationType="Catel.MVVM.Services.UIVisualizerService, Catel.MVVM" registrationType="Transient" />
          <register interfaceType="Catel.MVVM.Services.IProcessService, Catel.MVVM" implementationType="Catel.MVVM.Services.ProcessService, Catel.MVVM" />
        </serviceLocatorConfiguration>
        <serviceLocatorConfiguration name="test" supportDependencyInjection="false">
          <register interfaceType="Catel.MVVM.Services.IUIVisualizerService, Catel.MVVM" implementationType="Catel.MVVM.Services.Test.UIVisualizerService, Catel.MVVM" registrationType="Transient" />
          <register interfaceType="Catel.MVVM.Services.IProcessService, Catel.MVVM" implementationType="Catel.MVVM.Services.Test.ProcessService, Catel.MVVM" />
        </serviceLocatorConfiguration>
      </serviceLocatorConfigurations>
    </ioc>
  </catel>

  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
  </entityFramework>

  <connectionStrings>
    <add name="TestDbContextContainer" connectionString="metadata=res://*/TestDbContext.csdl|res://*/TestDbContext.ssdl|res://*/TestDbContext.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\SQLExpress;initial catalog=CatelUnitTestDbContext;Integrated Security=true&quot;" providerName="System.Data.EntityClient" />
    <add name="TestObjectContextContainer" connectionString="metadata=res://*/TestObjectContext.csdl|res://*/TestObjectContext.ssdl|res://*/TestObjectContext.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\SQLExpress;initial catalog=CatelUnitTestObjectContext;Integrated Security=true&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
</configuration>